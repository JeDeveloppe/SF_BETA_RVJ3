<!DOCTYPE html>

<title>Partenaire</title>

{% block body %}
    <h1>Partenaire</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ partenaire.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ partenaire.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ partenaire.description }}</td>
            </tr>
            <tr>
                <th>Collecte</th>
                <td>{{ partenaire.collecte }}</td>
            </tr>
            <tr>
                <th>Vend</th>
                <td>{{ partenaire.vend }}</td>
            </tr>
            <tr>
                <th>IsDon</th>
                <td>{{ partenaire.isDon ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ partenaire.url }}</td>
            </tr>
            <tr>
                <th>ImageBlob</th>
                <td>{{ partenaire.imageBlob }}</td>
            </tr>
            <tr>
                <th>IsDetachee</th>
                <td>{{ partenaire.isDetachee ? 'Yes' : 'No' }}</td>
            </tr>
