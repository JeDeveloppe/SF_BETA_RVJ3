                <th>IsComplet</th>
                <td>{{ partenaire.isComplet ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>IsEcommerce</th>
                <td>{{ partenaire.isEcommerce ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>IsOnLine</th>
                <td>{{ partenaire.isOnLine ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_partenaire_index') }}">back to list</a>

    <a href="{{ path('app_admin_partenaire_edit', {'id': partenaire.id}) }}">edit</a>

