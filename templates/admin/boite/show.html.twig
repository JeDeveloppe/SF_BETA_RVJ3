<!DOCTYPE html>

<title>Boite</title>

{% block body %}
    <h1>Boite</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ boite.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ boite.nom }}</td>
            </tr>
            <tr>
                <th>Editeur</th>
                <td>{{ boite.editeur }}</td>
            </tr>
            <tr>
                <th>Annee</th>
                <td>{{ boite.annee }}</td>
            </tr>
            <tr>
                <th>Imageblob</th>
                <td>{{ boite.imageblob }}</td>
            </tr>
            <tr>
                <th>Urlnom</th>
                <td>{{ boite.urlnom }}</td>
            </tr>
            <tr>
                <th>IsLivrable</th>
                <td>{{ boite.isLivrable ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>IsComplet</th>
                <td>{{ boite.isComplet ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>PoidBoite</th>
                <td>{{ boite.poidBoite }}</td>
            </tr>
            <tr>
                <th>Age</th>
                <td>{{ boite.age }}</td>
            </tr>
            <tr>
                <th>NbrJoueurs</th>
                <td>{{ boite.nbrJoueurs }}</td>
            </tr>
            <tr>
                <th>PrixHt</th>
                <td>{{ boite.prixHt }}</td>
            </tr>
            <tr>
                <th>IsDeee</th>
                <td>{{ boite.isDeee ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Contenu</th>
                <td>{{ boite.contenu }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ boite.message }}</td>
            </tr>
            <tr>
                <th>IsOnLine</th>
                <td>{{ boite.isOnLine ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ boite.createdAt ? boite.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_boite_index') }}">back to list</a>

    <a href="{{ path('app_boite_edit', {'id': boite.id}) }}">edit</a>

    {{ include('boite/_delete_form.html.twig') }}
{% endblock %}
