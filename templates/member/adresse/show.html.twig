<!DOCTYPE html>

<title>Adresse</title>

{% block body %}
    <h1>Adresse</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ adresse.id }}</td>
            </tr>
            <tr>
                <th>LastName</th>
                <td>{{ adresse.lastName }}</td>
            </tr>
            <tr>
                <th>FirstName</th>
                <td>{{ adresse.firstName }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ adresse.adresse }}</td>
            </tr>
            <tr>
                <th>IsFacturation</th>
                <td>{{ adresse.isFacturation ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>IsLivraison</th>
                <td>{{ adresse.isLivraison ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_adresse_index') }}">back to list</a>

    <a href="{{ path('app_adresse_edit', {'id': adresse.id}) }}">edit</a>

    {{ include('adresse/_delete_form.html.twig') }}
{% endblock %}
